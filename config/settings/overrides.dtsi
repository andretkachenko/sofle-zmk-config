// overrides of standard settings, keyboard-agnostic
#include <behaviors/num_word.dtsi>

&sk {  // sticky-key config
    release-after-ms = <900>;  // release after 0.9s
    quick-release;             // no double capitalization when rolling keys
};

&lt {
    tapping-term-ms = <250>;
};

&num_word {
    ignore-modifiers;
};