/ {
    keymap {
        compatible = "zmk,keymap";
        
        uni {
            display-name = "uni";
            bindings = <
&kp ESC        &kp N1         &kp N2        &kp N3        &kp N4          &kp N5                                        &kp N6          &kp N7          &kp N8        &kp N9          &kp N0            &kp BSPC
&kp GRAVE      &kp Q          &kp W         &kp E         &kp R           &kp T                                         &kp Y           &kp U           &kp I         &kp O           &kp P             &kp LBKT
&kp TAB        &kp A          &kp S         &kp D         &kp F           &kp G                                         &kp H           &kp J           &kp K         &kp L           &kp SEMI          &kp SQT
&kp LSHFT      &kp Z          &kp X         &kp C         &kp V           &kp B           &kp F24         &kp F23       &kp N           &kp M           &kp COMMA     &kp DOT         &kp FSLH          &kp RSHFT 
                              &kp LGUI      &kp LALT      &kp LCTRL       &kp SPACE       &mo FN          &kp RET       &kp LEFT        &kp UP          &kp DOWN      &kp RIGHT
            >;

            sensor-bindings = <&inc_dec_kp LA(TAB) LS(LA(TAB)) &inc_dec_kp LC(Z) LC(Y)>;
        };
    };
};